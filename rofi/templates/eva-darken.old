! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2b4458, #2b4458, #193145

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2b4458, #a6a6a6, #2b4458, #2b4458, #ffffff
rofi.color-active:  #2b4458, #16d2ce, #2b4458, #2b4458, #16d2ce
rofi.color-urgent:  #2b4458, #df676a, #2b4458, #2b4458, #df676a

! Set the desired separator style
rofi.separator-style: solid
